	<parameter name="idOrdenpago" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select ordenes_pago.fechaOrdenpago, ordenes_pago.idOrdenpago, clientes.idCliente,
concat(clientes.nomCliente," ",clientes.apellCliente),
ordenes_pago.tipopagoOrdenpago, productos.idProducto, productos.nomProducto, productos.descriProducto,
productos_ordenes.idTalla, productos_ordenes.cantidad, productos.precioProducto, productos_ordenes.total, 
ordenes_pago.valorTotalOrdenpago
from clientes inner join ordenes_pago
on clientes.idCliente = ordenes_pago.idCliente 
inner join productos_ordenes on
ordenes_pago.idOrdenpago = productos_ordenes.idOrdenpago
inner join productos
on productos_ordenes.idProducto = productos.idProducto  
where ordenes_pago.idOrdenpago = $P{idOrdenpago};]]>
	</queryString>
	<field name="fechaOrdenpago" class="java.sql.Date">
		<fieldDescription><![CDATA[Hace referencia a la fecha (año, mes, dia) en que se realiza la orden de pago.]]></fieldDescription>
	</field>
	<field name="idOrdenpago" class="java.lang.Integer">
		<fieldDescription><![CDATA[Clave primaria de la tabla ordenes de pago, esta identifica las ordenes de pago (facturas) que son generadas en el sistema de información.]]></fieldDescription>
	</field>
	<field name="idCliente" class="java.lang.Integer">
		<fieldDescription><![CDATA[Clave primaria de la tabla clientes, hace referencia al número único de identificación (Cédula) que posee cada cliente.]]></fieldDescription>
	</field>
	<field name="concat(clientes.nomCliente,&quot; &quot;,clientes.apellCliente)" class="java.lang.String"/>
	<field name="tipopagoOrdenpago" class="java.lang.String">
		<fieldDescription><![CDATA[Hace referencia al método de pago que drealiza el cliente al momento de la compra de los productos.]]></fieldDescription>
	</field>
	<field name="idProducto" class="java.lang.Integer">
		<fieldDescription><![CDATA[Clave primaria de la tabla producto, esta identifica los productos que comercializa la empresa.]]></fieldDescription>
	</field>
	<field name="nomProducto" class="java.lang.String">
		<fieldDescription><![CDATA[Hace referencia al nombre del producto.]]></fieldDescription>
	</field>
	<field name="descriProducto" class="java.lang.String">
		<fieldDescription><![CDATA[Hace referencia a una descripción corta del producto, donde se puede mencionar las características que puede tener el producto.]]></fieldDescription>
	</field>
	<field name="idTalla" class="java.lang.String">
		<fieldDescription><![CDATA[Hace referencia a la talla del producto comprado.]]></fieldDescription>
	</field>
	<field name="cantidad" class="java.lang.Integer">
		<fieldDescription><![CDATA[Hace referencia a la cantidad que se realizará del producto.]]></fieldDescription>
	</field>
	<field name="precioProducto" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[Hace referencia al precio, valor o costo del producto en forma individual.]]></fieldDescription>
	</field>
	<field name="total" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="valorTotalOrdenpago" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[Hace referencia a la sumatoria total en la compra de los productos.]]></fieldDescription>
	</field>
